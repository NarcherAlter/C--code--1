#include <stdio.h>  //代码块                  //printf打印函数，scanf输入函数

void one_three();
void two();
int main(void)
{	
	printf("starting now\n");
	one_three();
	printf("done!");
	return 0;
}
void one_three()
{
	printf("one\n");
	two();
	printf("three\n");
}
void two()
{
	printf("two\n");
}
